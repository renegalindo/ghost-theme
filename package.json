{
    "name": "rene",
    "description": "A minimal theme designed for Rene",
    "version": "4.1.0",
    "engines": {
        "ghost": ">=4.0.0"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "pretest": "gulp build",
        "test": "gscan .",
        "pretest:ci": "gulp build",
        "test:ci": "gscan --fatal --verbose ."
    },
    "author": {
        "name": "Vikas Potluri",
        "email": "vikaspotluri123.github@gmail.com",
        "url": "https://vikaspotluri.me"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/renegalindo/rene-blog-theme.git"
    },
    "devDependencies": {
        "autoprefixer": "10.2.5",
        "beeper": "2.1.0",
        "cssnano": "4.1.10",
        "gscan": "4.43.1",
        "gulp": "4.0.2",
        "gulp-concat": "2.6.1",
        "gulp-livereload": "4.0.2",
        "gulp-postcss": "9.0.0",
        "gulp-uglify": "3.0.2",
        "gulp-zip": "5.1.0",
        "postcss": "8.2.8",
        "postcss-color-mod-function": "3.0.3",
        "postcss-easy-import": "3.0.0",
        "pump": "3.0.0"
    },
    "browserslist": [
        "defaults"
    ],
    "config": {
        "posts_per_page": 1
